<div class="accordion">
  {% for item in items %}
  <div class="accordion-item">
    <button class="accordion-header" data-toggle="accordion">
      {{ item.title }}
      <span class="accordion-icon">+</span>
    </button>
    <div class="accordion-content">
      {{ item.content|safe }}
    </div>
  </div>
  {% endfor %}
</div>